            <script>



                document.addEventListener('DOMContentLoaded', function () {
                    // Check if URL contains 'success' attribute
                    const urlParams = new URLSearchParams(window.location.search);
                    const success = urlParams.get('success');

                    if (success === 'true') {
                        // Display the popup card
                        const popupCard = document.getElementById('popup');
                        const shade = document.getElementById('black-shade');
                        popupCard.style.display = 'block';
                        shade.style.display = 'block';

                        const closeButton = document.getElementById('close');
                        closeButton.addEventListener('click', function () {
                            window.location.href = '/';
                        })
                    }
                });

                $(document).ready(function () {
                    var courses = [
                        { name: "BBA", url: "/bba.html" },
                        { name: "BA", url: "/ba.html" },
                        { name: "BCA", url: "/bca.html" },
                        { name: "MBA", url: "/mba.html" },
                        { name: "MA", url: "/ma.html" },
                        { name: "MCA", url: "/mca.html" }
                    ];

                    $('#course-input').on('input', function () {
                        var keyword = $(this).val();

                        var suggestions = courses.filter(function (course) {
                            return course.name.toLowerCase().includes(keyword.toLowerCase());
                        });

                        if (suggestions.length > 0) {
                            $('#course-suggestions').empty();
                            $.each(suggestions, function (index, suggestion) {
                                var suggestionItem = $('<a class="dropdown-item" href="' + suggestion.url + '">' + suggestion.name + '</a>');
                                $('#course-suggestions').append(suggestionItem);
                            });
                            $('#course-suggestions').show();
                        } else {
                            $('#course-suggestions').hide();
                        }
                    });

                    // Delay the hiding of suggestions to allow link clicks
                    $('#course-input').on('blur', function () {
                        setTimeout(function () {
                            $('#course-suggestions').hide();
                        }, 200);
                    });

                    // Keep the suggestions visible while hovering over them
                    $('#course-suggestions').on('mousedown', function (event) {
                        event.preventDefault(); // Prevent blur event when selecting
                    });
                });

            </script>

